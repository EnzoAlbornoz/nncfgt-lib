{
    "env": {
        "commonjs": true,
        "es6": true,
		"node": true
    },
    "extends": [
		"airbnb-base",
		"plugin:prettier/recommended"
	],
	"plugins": [
		"prettier"
	],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
		"ecmaVersion": 2020,
		"sourceType": "script"
    },
    "rules": {
		"prettier/prettier": ["error"],
		"strict": ["error", "global"]
    }
}
